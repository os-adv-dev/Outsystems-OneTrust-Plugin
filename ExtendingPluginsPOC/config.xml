<?xml version='1.0' encoding='utf-8'?>
<widget id="com.outsystems.experts.ExtendingPluginsPOC" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloCordova</name>
    <description>
        Native application generated by OutSystems MiniMabs
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        OutSystems Inc.
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="AndroidXEnabled" value="true" />
        <preference name="AndroidInsecureFileModeEnabled" value="true" />
        <allow-intent href="market:*" />
        <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application">
            <application android:usesCleartextTraffic="true" />
        </edit-config>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="deployment-target" value="12.0" />
    </platform>
    <plugin name="com.chuckytuh.cordova.rsswizzle" spec="https://github.com/OutSystems/cordova-plugin-rsswizle.git#1.1.0" />
    <plugin name="cordova-sqlite-storage" spec="https://github.com/OutSystems/cordova-sqlite-storage.git#3.2.0-OS3" />
    <plugin name="@outsystems/cordova-outsystems-wkwebview" spec="file:./plugins//@outsystems/cordova-outsystems-wkwebview" />
    <plugin name="@outsystems/cordova-outsystems-androidx" spec="file:./plugins//@outsystems/cordova-outsystems-androidx" />
</widget>
